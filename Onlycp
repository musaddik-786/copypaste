                        subprocess.Popen(
                            [sys.executable, agent_main],
                            cwd=agent_dir,
                            stdout=subprocess.DEVNULL,
                            stderr=subprocess.DEVNULL,
                        )
                        # small pause to allow server to begin booting
                        time.sleep(10)
                except Exception as e:
                    print(f"âš ï¸ Failed to start Data Extraction Agent: {e}")
