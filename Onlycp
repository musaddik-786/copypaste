# db_path = '../mounted_data/app_db/chat_history.db'

app_name = os.environ.get("APP_NAME")

DEFAULT_USER_ID = os.environ.get("DEFAULT_USER","DEFAULT") 
DEFAULT_SESSION_ID = os.environ.get("DEFAULT_SESSION","DEFAULTSESSION1")

